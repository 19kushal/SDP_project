<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Residents</title>
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.1.3/dist/css/bootstrap.min.css"
        integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <!-- google fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Gotu&display=swap" rel="stylesheet">
    <!-- internal css  -->
    <style>
        *{
            box-sizing: border-box;
            padding: 0;
            margin: 0;
        }
        body{
            color: white;
           background-color: #00374f;
           overflow: hidden;
        }
        .navbar{
            color: #00ED64 !important;
            background-color: #001E2B;
        }
        .nav-link{
            padding: 0 2rem !important;
            color: #00ED64 !important;
        }
        .nav-link:hover{
            color: #ffffff !important;
          }
        .navbar-brand{
            font-weight: 600 !important;
        }
        .section-head{
            background-color: #00374f;
            padding: 2rem;
            text-align: center;
        }
        .row{
            display: flex;
            align-items: center;
            justify-content: center;
        }
        .col-2{
            border: 1px solid white;
            display: flex;
            align-items: center;
            justify-content: center;

        }
    </style>

</head>

<body>
     <!-- navbar  -->
     <nav class="navbar navbar-expand-lg navbar-dark" id="navflex">
        <div class="nav-left">
          <a class="navbar-brand" href="#">Scrapers.</a>
          <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
          </button>
        </div>
        <div class="collapse navbar-collapse nav-right" id="navbarNavAltMarkup">
          <div class="navbar-nav ">
            <a class="nav-item nav-link active" href="#profile">Profile <span class="sr-only">(current)</span></a>
            <a class="nav-item nav-link" href="#anna">Announcement</a>
            <a class="nav-item nav-link" href="#notice">Notice</a>
            <a class="nav-item nav-link" href="#complaint">Complaints</a>
          </div>
        </div>
      </nav>
    <div class="section-head"><h1>Residents</h1></div>
    <section id="resi">
        <div class="container">

            <div class="row">
                <div class="col-2">Name</div>
                <div class="col-2">Contact</div>
                <div class="col-2">Wing</div>
                <div class="col-2">Floor</div>
                <div class="col-2">Flat</div>
                <div class="col-2">Type of Resident</div>
            </div> 
            <% users.forEach(users=>{ %>
                <div class="row">
                    <div class="col-2"> <%= users.name %></div>
                    <div class="col-2"> <%= users.contact %></div>
                    <div class="col-2"> <%= users.wing %></div>
                    <div class="col-2"> <%= users.floor %></div>
                    <div class="col-2"> <%= users.flat %></div>
                    <div class="col-2"> <%= users.resiType %></div>
                </div>
                <% }) %>
            </div>
    </section>
       
</body>

</html>